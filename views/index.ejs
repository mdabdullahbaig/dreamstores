<div class="container" style="padding:12px"> 
        <header class="jumbotron">

                <h4 style="color:brown"> <strong>DREAM</strong>STORE,Not just a Store </h4>
            
                <% if(currentUser && currentUser.isSeller){%>
                    <div class="container">
                            
                        <a href="/home/new"> <button class="btn bnt-default btn-lg" style="color: brown"><strong>Add Product</strong></button></a>
                        <a href="/home/currentseller/profile"> <button class="btn bnt-default btn-lg" style="color: brown; float:right; "><strong>Seller Details</strong></button></a>
                        
                    </div>    
                <%} %>
            
        </header>


        <div class="row text-center" style="display:flex;flex-wrap:wrap;">

                <div class="col-md-12">
                        <% if(noMatch !== null) { %>
                        <h3  style="color:brown" ><%= noMatch %></h3>
                        <% } %>
                </div>       
                <% carts.forEach(function(cart){%>
                  <div class="col-md-3 col-sm-6">
                        <div class="thumbnail">
                                <img src="<%= cart.image %>">
                                
                            <div class="caption">
                                    <h4> <%= cart.product %> </h4>
                            </div>
                            <p>
                                <a href="/home/<%= cart._id %>" class="btn btn-info btn-sm">More info</a>
                            </p>
                        </div>
                      
                      
                  </div>
                  
                <%});  %>
        </div>
                    

</div>






